{
  "name": "sol-swap",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-local": "vite --port 3000",
    "build": "tsc && vite build",
    "check-types": "tsc --noEmit --pretty",
    "lint": "eslint . \"src/**/*.{js,jsx,ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "prettier:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "prettier:write": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "preview": "vite preview",
    "prepare": "husky install"
  },
  "dependencies": {
    "@radix-ui/themes": "^2.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-router-dom": "^6.21.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.7",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
